{
  "name": "autobahnTS",
  "version": "0.9.9",
  "description": "An implementation of The Web Application Messaging Protocol (WAMP).",
  "main": "index.js",
  "browser": {
    "./src/transport/rawsocket": false,
    "ws": false,
    "crypto-js": false
  },
  "scripts": {
    "test": "nodeunit test/test.js",
    "postinstall": "npm run typings",
    "tsc": "tsc",
    "typings": "typings install",
    "watch": "tsc --watch",
    "minify": "uglifyjs -o build/autobahn.min.js build/autobahn.js",
    "bundle": "browserify src/autobahn.js --standalone autobahn -o build/autobahn.js",
    "build": "npm run tsc && npm run bundle && npm run minify"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "bufferutil": "^1.2.1",
    "crypto-js": ">= 3.1.5",
    "nodeunit": "^0.9.1",
    "typescript": "^1.7.5",
    "typings": "^0.4.1",
    "uglify-js": "^2.6.1",
    "utf-8-validate": "^1.2.1",
    "when": ">= 3.7.3",
    "ws": ">= 0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/crossbario/autobahn-js.git"
  },
  "keywords": [
    "WAMP",
    "WebSocket",
    "RPC",
    "PubSub"
  ],
  "author": "Tavendo GmbH",
  "license": "MIT"
}
